@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Tablas del proyecto</h1>
    <p class="lead">2do Avance del proyecto.</p>


    <div id="carousel-example-generic" class="carousel slide carousel-fade" data-ride="carousel" data-interval="3000" data-pause="null">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
            <li data-target="#carousel-example-generic" data-slide-to="1"></li>
            <li data-target="#carousel-example-generic" data-slide-to="2"></li>
            <li data-target="#carousel-example-generic" data-slide-to="3"></li>
            <li data-target="#carousel-example-generic" data-slide-to="4"></li>
            <li data-target="#carousel-example-generic" data-slide-to="5"></li>
            <li data-target="#carousel-example-generic" data-slide-to="6"></li>
            <li data-target="#carousel-example-generic" data-slide-to="7"></li>
            <li data-target="#carousel-example-generic" data-slide-to="8"></li>
            <li data-target="#carousel-example-generic" data-slide-to="9"></li>
            <li data-target="#carousel-example-generic" data-slide-to="10"></li>
            <li data-target="#carousel-example-generic" data-slide-to="11"></li>
            <li data-target="#carousel-example-generic" data-slide-to="12"></li>
            <li data-target="#carousel-example-generic" data-slide-to="13"></li>
        </ol>

        <!-- Wrapper for slides -->
        <!-- slide 1 -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="~/Images/Countries.jpeg"/>
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">@Html.ActionLink("Países", "Index", "Countries", null,
                                                new { @class = "routeStyle" })</h1>
                    <p class="super-paragraph">Lista de losPaises .</p>
                </div>
            </div>
            <!-- slide 2 -->
            <div class="item">
                <img src="~/Images/Customers.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Cliente</h1>
                    <p class="super-paragraph">Lista de clientes.</p>
                </div>
            </div>
            <!-- slide 3 -->
            <div class="item">
                <img src="~/Images/Departments.png" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Departamentos</h1>
                    <p class="super-paragraph">Lista de Departamentos.</p>
                </div>
            </div>
            <!-- slide 4 -->
            <div class="item">
                <img src="~/Images/Employees.jpeg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Empleados</h1>
                    <p class="super-paragraph">Lista de empleados.</p>
                </div>
            </div>
            <!-- slide 5 -->
            <div class="item">
                <img src="~/Images/Inventory.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Inventario</h1>
                    <p class="super-paragraph">EL inventario de la organizacion.</p>
                </div>
            </div>
            <!-- slide 6 -->
            <div class="item">
                <img src="~/Images/Job.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Trabajo</h1>
                    <p class="super-paragraph">trabajos realizados.</p>
                </div>
            </div>

            <!-- slide 7 -->
            <div class="item">
                <img src="~/Images/JobHistory.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Historial de Trabajo</h1>
                    <p class="super-paragraph">trabajos.</p>
                </div>
            </div>

            <!-- slide 8 -->
            <div class="item">
                <img src="~/Images/Location.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">
                        @Html.ActionLink("Locaciones", "Index", "Locations", null,
            new { @class = "routeStyle" })
                    </h1>
                    <p class="super-paragraph">COORDENADAS.</p>
                </div>
            </div>

            <!-- slide 9 Orders-->
            <div class="item">
                <img src="~/Images/Order.png" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Pedidos</h1>
                    <p class="super-paragraph">Ordenes de compras.</p>
                </div>
            </div>

            <!-- slide 10  OrdersItem-->
            <div class="item">
                <img src="~/Images/OrdersItem.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Articulo Pedido</h1>
                    <p class="super-paragraph">Item solicitado en una orden de compra</p>
                </div>
            </div>

            <!-- slide 11 ProductDescription-->
            <div class="item">
                <img src="~/Images/ProductDescription.png" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">
                        @Html.ActionLink("Descripcion Del producto", "Index", "ProductDescriptions", null,
           new { @class = "routeStyle" })
                    </h1>
                    <p class="super-paragraph">Descripciones.</p>
                </div>
            </div>

            <!-- slide 12 -->
            <div class="item">
                <img src="~/Images/ProductInformation.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">@Html.ActionLink("InformacionesDeProducto", "Index", "ProductInformations", null,
                                                 new { @class = "routeStyle" })
                    </h1>
                    <p class="super-paragraph">Infos.</p>
                </div>
            </div>

            <!-- slide 13 Regions -->
            <div class="item">
                <img src="~/Images/regions.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">
                        @Html.ActionLink("Regiones", "Index", "Regions", null,
            new { @class = "routeStyle" })
                                    </h1>
                    <p class="super-paragraph">Subdivision De La
                    Empresa.</p>
                </div>
            </div>

            <!-- slide 14 -->
            <div class="item">
                <img src="~/Images/warehouse.jpg" />
                <div class="overlay"></div>
                <div class="carousel-caption">
                    <h1 class="super-heading">Deposito</h1>
                    <p class="super-paragraph">Deposito de todo el inventario.</p>
                </div>
            </div>

        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

